<local:BasePage x:Class="SeaBattleWPF.Pages.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SeaBattleWPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SettingsPage"
                DataContext="{Binding SettingsViewModel , Source={StaticResource ViewModelLocator}}">

    <Grid>
        <StackPanel>
            <Button Style="{StaticResource StyledButton}" Content="Go back" Command="{Binding GoBackCommand}"  ></Button>
        </StackPanel>
    </Grid>
</local:BasePage>
